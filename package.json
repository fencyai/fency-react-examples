{
    "name": "fency-react-example",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "build:watch": "tsc -b --watch",
        "lint": "eslint .",
        "preview": "vite preview",
        "ci": "npm run lint && npm run build",
        "check-file-refs": "tsx scripts/check-file-refs.ts",
        "set-local-deps": "tsx scripts/set-local-deps.ts && npm link @fencyai/js @fencyai/react && npm install",
        "set-remote-deps": "npm unlink @fencyai/js @fencyai/react && npm cache clean --force && npm install @fencyai/js@latest @fencyai/react@latest",
        "compile": "tsc -b --noEmit"
    },
    "dependencies": {
        "@fencyai/js": "^0.1.75",
        "@fencyai/react": "^0.1.75",
        "@hookform/resolvers": "^5.2.2",
        "@mantine/code-highlight": "^8.2.8",
        "@mantine/core": "^8.2.8",
        "@mantine/hooks": "^8.2.8",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-scroll-area": "^1.2.10",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slot": "^1.2.3",
        "@tabler/icons-react": "^3.34.1",
        "@tailwindcss/vite": "^4.1.11",
        "@uppy/aws-s3": "^5.0.1",
        "@uppy/core": "^5.0.2",
        "@uppy/dashboard": "^5.0.4",
        "@uppy/react": "^5.1.1",
        "ai": "^6.0.0",
        "axios": "^1.11.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-react": "^0.546.0",
        "nanoid": "^5.1.6",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.63.0",
        "react-router": "^7.6.3",
        "shiki": "^3.12.2",
        "streamdown": "^1.3.0",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.11",
        "use-stick-to-bottom": "^1.1.1",
        "uuid": "^13.0.0",
        "zod": "^4.0.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@types/node": "^24.3.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^5.0.0",
        "eslint": "^9.30.1",
        "eslint-plugin-react-hooks": "^7.0.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.3.0",
        "husky": "^9.0.11",
        "lint-staged": "^16.0.0",
        "postcss": "^8.5.6",
        "postcss-preset-mantine": "^1.18.0",
        "postcss-simple-vars": "^7.0.1",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.6",
        "tw-animate-css": "^1.3.7",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.35.1",
        "vite": "^7.0.4"
    },
    "lint-staged": {
        "package.json": "npm run check-file-refs"
    }
}
